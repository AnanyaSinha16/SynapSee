# Choose AI mode: OPENAI | GEMINI | OLLAMA | NONE
AI_MODE=OPENAI

# OpenAI
OPENAI_API_KEY=replace_with_openai_key
OPENAI_MODEL=gpt-4o-mini

# Gemini (requires OAuth access token or service account flow)
GEMINI_ACCESS_TOKEN=replace_with_gemini_oauth_token
GEMINI_MODEL=gemini-pro

# Ollama (local LLM) - if using ollama on your laptop
OLLAMA_URL=http://localhost:11434

# Server port
PORT=4000
